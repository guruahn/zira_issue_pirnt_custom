{
    "manifest_version": 3,
    
    "name": "Zira Print Style Custom",
    "description": "change font-family for print",
    "version": "1.0",
    "manifest_version": 2,
    "background" : {
       "scripts": ["background.js"],
       "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["http://*.atlassian.net/*", "https://*.atlassian.net/*", "<all_urls>"],
            "match_about_blank": true,
            "css": ["zira_print.css"]
            //"js": ["webfont.js","content.js"]
        }
    ],
    "page_action": {
        "default_icon": "icon.png",
        "default_title": "Zira Print Style Custom"
    },
    "permissions": [ "declarativeContent", "fontSettings" ]
}